{
  "private": true,
  "name": "@theia/example-browser",
  "version": "0.7.2",
  "license": "EPL-2.0 OR GPL-2.0 WITH Classpath-exception-2.0",
  "theia": {
    "frontend": {
      "config": {
        "applicationName": "Theia Browser Example",
        "preferences": {
          "files.enableTrash": false
        }
      }
    }
  },
  "dependencies": {
    "@theia/callhierarchy": "^0.7.2",
    "@theia/console": "^0.7.2",
    "@theia/core": "^0.7.2",
    "@theia/cpp": "^0.7.2",
    "@theia/debug": "^0.7.2",
    "@theia/debug-nodejs": "^0.7.2",
    "@theia/editor": "^0.7.2",
    "@theia/editor-preview": "^0.7.2",
    "@theia/editorconfig": "^0.7.2",
    "@theia/file-search": "^0.7.2",
    "@theia/filesystem": "^0.7.2",
    "@theia/getting-started": "^0.7.2",
    "@theia/git": "^0.7.2",
    "@theia/java": "^0.7.2",
    "@theia/java-debug": "^0.7.2",
    "@theia/json": "^0.7.2",
    "@theia/keymaps": "^0.7.2",
    "@theia/languages": "^0.7.2",
    "@theia/markers": "^0.7.2",
    "@theia/merge-conflicts": "^0.7.2",
    "@theia/messages": "^0.7.2",
    "@theia/metrics": "^0.7.2",
    "@theia/mini-browser": "^0.7.2",
    "@theia/monaco": "^0.7.2",
    "@theia/navigator": "^0.7.2",
    "@theia/outline-view": "^0.7.2",
    "@theia/output": "^0.7.2",
    "@theia/plugin-ext": "^0.7.2",
    "@theia/plugin-ext-vscode": "^0.7.2",
    "@theia/preferences": "^0.7.2",
    "@theia/preview": "^0.7.2",
    "@theia/process": "^0.7.2",
    "@theia/python": "^0.7.2",
    "@theia/scm": "^0.7.2",
    "@theia/search-in-workspace": "^0.7.2",
    "@theia/task": "^0.7.2",
    "@theia/terminal": "^0.7.2",
    "@theia/textmate-grammars": "^0.7.2",
    "@theia/tslint": "^0.7.2",
    "@theia/typehierarchy": "^0.7.2",
    "@theia/typescript": "^0.7.2",
    "@theia/userstorage": "^0.7.2",
    "@theia/variable-resolver": "^0.7.2",
    "@theia/workspace": "^0.7.2"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn build",
    "clean": "theia clean && rimraf errorShots",
    "build": "theiaext compile && theia build --mode development",
    "watch": "concurrently -n compile,bundle \"theiaext watch --preserveWatchOutput\" \"theia build --watch --mode development\"",
    "start": "theia start --plugins=local-dir:../../plugins",
    "start:debug": "yarn start --log-level=debug",
    "test": "wdio wdio.conf.js",
    "test-non-headless": "wdio wdio-non-headless.conf.js",
    "coverage:compile": "yarn build --config coverage-webpack.config.js",
    "coverage:remap": "remap-istanbul -i coverage/coverage.json -o coverage/coverage-final.json --exclude 'frontend/index.js' && rimraf coverage/coverage.json",
    "coverage:report:html": "istanbul report --root coverage --format html",
    "coverage:report:lcov": "istanbul report --root coverage --format lcov",
    "coverage": "yarn coverage:compile && yarn test && yarn coverage:remap && yarn coverage:report:lcov && yarn coverage:report:html"
  },
  "devDependencies": {
    "@theia/cli": "^0.7.2"
  }
}
